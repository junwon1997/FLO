<?xml version="1.0" encoding="utf-8"?>
<com.sothree.slidinguppanel.SlidingUpPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:id="@+id/main_frame"
    android:gravity="bottom"
    app:umanoPanelHeight="190dp"
    app:umanoShadowHeight="0dp"
    app:umanoFadeColor="@color/transparent"
    app:umanoDragView="@id/slide_layout"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/back_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/album_back_ib"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="40dp"
            android:layout_marginStart="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/btn_arrow_black"
            android:background="@color/white"
            android:scaleType="fitXY"/>

        <ImageButton
            android:id="@+id/album_more_ib"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/btn_player_more"
            android:background="@color/white"
            android:scaleType="fitXY"/>

        <ImageButton
            android:id="@+id/album_like_ib"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="15dp"
            app:layout_constraintEnd_toStartOf="@id/album_more_ib"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/ic_my_like_off"
            android:background="@color/white"
            android:scaleType="fitXY"/>

        <ImageButton
            android:id="@+id/album_like_on_ib"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="15dp"
            app:layout_constraintEnd_toStartOf="@id/album_more_ib"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/ic_my_like_on"
            android:background="@color/white"
            android:scaleType="fitXY"
            android:visibility="gone"
            />



        <TextView
            android:id="@+id/album_title_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="IU 5th Album 'LILAC'"
            android:textSize="25sp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:gravity="center"
            android:layout_marginTop="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/album_back_ib"
            app:layout_constraintBottom_toTopOf="@id/album_singer_tv"/>

        <TextView
            android:id="@+id/album_singer_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="아이유(IU)"
            android:textSize="15sp"
            android:textColor="@color/gray"
            android:gravity="center"
            android:layout_marginTop="5dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/album_title_tv"
            app:layout_constraintBottom_toTopOf="@id/album_information_tv"/>

        <TextView
            android:id="@+id/album_information_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="2021.03.25 | 정규 |댄스 팝"
            android:textSize="15sp"
            android:textColor="@color/gray"
            android:gravity="center"
            android:layout_marginTop="5dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/album_singer_tv"
            app:layout_constraintBottom_toTopOf="@id/album_img_fl"/>


        <FrameLayout
            android:id="@+id/album_img_fl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/album_information_tv"
            android:layout_marginTop="10dp"
            >
            <androidx.cardview.widget.CardView
                android:id="@+id/album_img_cv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="10dp"
                app:cardElevation="0dp"

                >
                <ImageView
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:src="@drawable/img_album_exp2"
                    android:scaleType="fitXY"/>
            </androidx.cardview.widget.CardView>

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="right|bottom"
                android:src="@drawable/widget_black_play"
                />

        </FrameLayout>

        <ImageView
            android:id="@+id/album_img_lp_iv"
            android:layout_width="40dp"
            android:layout_height="180dp"
            android:src="@drawable/img_album_lp"
            android:scaleType="fitXY"
            app:layout_constraintTop_toTopOf="@id/album_img_fl"
            app:layout_constraintBottom_toBottomOf="@id/album_img_fl"
            app:layout_constraintStart_toEndOf="@id/album_img_fl"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/slide_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/album_total_song_cl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:background="@drawable/select_box"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="수록곡"
                android:background="@drawable/select_change_box"
                android:textColor="@color/purple_500"
                android:textStyle="bold"
                android:textSize="18sp"
                android:padding="15dp"
                android:layout_marginStart="40dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="상세정보"
                android:textColor="@color/gray"
                android:textStyle="bold"
                android:textSize="18sp"
                android:padding="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="영상"
                android:textColor="@color/gray"
                android:textStyle="bold"
                android:textSize="18sp"
                android:padding="15dp"
                android:layout_marginEnd="40dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>




        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/album_toggle_box_cl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@id/album_total_song_cl"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:padding="10dp"
            android:background="@drawable/toggle_box">

            <TextView
                android:id="@+id/album_toggle_box_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="15sp"
                android:text="내 취향 MIX"
                android:layout_marginStart="5dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <ImageView
                android:id="@+id/album_toggle_box_iv"
                android:layout_width="40dp"
                android:layout_height="20dp"
                android:src="@drawable/btn_toggle_off"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/album_toggle_box_tv"/>

            <ImageView
                android:id="@+id/album_toggle_change_box_iv"
                android:layout_width="40dp"
                android:layout_height="20dp"
                android:src="@drawable/btn_toggle_on"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/album_toggle_box_tv"
                android:visibility="gone"/>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/album_select_cl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/album_toggle_box_cl"
            >


            <ImageView
                android:id="@+id/album_select_iv"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/btn_playlist_select_off"
                android:layout_marginStart="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>



            <TextView
                android:id="@+id/album_select_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="전체선택"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@id/album_select_iv"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />


            <TextView
                android:id="@+id/album_arrow_right_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="전체선택"
                android:textSize="12sp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginEnd="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                />

            <ImageView
                android:id="@+id/album_arrow_right_iv"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/icon_browse_arrow_right"
                app:layout_constraintEnd_toStartOf="@id/album_arrow_right_tv"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>


        <ScrollView
            android:id="@+id/album_playlist_sv"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintVertical_weight="1"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="20dp"
            android:scrollbars="none"
            android:overScrollMode="never"
            app:layout_constraintTop_toBottomOf="@id/album_select_cl"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginBottom="20dp">

